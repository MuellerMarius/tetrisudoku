{
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended"],
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true
  },
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ],
    "linebreak-style": 0,
    "react/jsx-props-no-spreading": "off"
  }
}
